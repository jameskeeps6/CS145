    public LetterInv add (LetterInv other) {
    	//Create new object
    	LetterInv temp = new LetterInv();
    	temp.input_char_array[0] = ' ';
    	//Loop thru word and add to new word
    	for(int i = 0; i < input_char_array.length; i++) {
    		//Add letter count
    		temp.input_char_array[i] = (char) (input_char_array[i] + other.input_char_array[i]);
    	}
    	//return new object
    	return temp;

    	}
